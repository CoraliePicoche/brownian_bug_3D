\begin_layout Subsection
Dominance index
\end_layout

\begin_layout Standard
In the Poisson point process, 
\begin_inset Formula $K_{ii}(r)=K_{ij}(r)$
\end_inset

, which means that the dominance index can be reduced to ratios of concentration
s:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathcal{D}_{i}(r)=\frac{C_{i}}{\sum_{j=1}^{S}C_{j}}.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
In the Thomas process, using eq.
 
\begin_inset CommandInset ref
LatexCommand ref
reference "eq:k_thomas"
plural "false"
caps "false"
noprefix "false"

\end_inset

, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mathcal{D}_{i}(r)=\frac{C_{i}\left(\frac{4}{3}\pi r^{3}+\frac{F(r)}{C_{p,i}}\right)}{C_{i}\frac{F(r)}{C_{p,i}}+\sum_{j}C_{j}\frac{4}{3}\pi r^{3}}.
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
with 
\begin_inset Formula $F(r)=\frac{1}{\sigma\sqrt{\pi}}\left(\sigma\sqrt{\pi}\erf\left(\frac{r}{2\sigma}\right)-re^{-\left(\frac{r}{2\sigma}\right)^{2}}\right)$
\end_inset

.
\end_layout

