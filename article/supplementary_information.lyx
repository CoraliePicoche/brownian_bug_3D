\begin_inset Formula 
\begin{equation}
\begin{array}{ccc}
Pr(\min(R_{1},...,R_{n})>r) & = & Pr(R_{1}>r,...,R_{n}>r)\\
 & = & \Pi_{i}Pr(R_{i}>r)\\
 & = & \Pi_{i}\exp(-b_{d}r^{_{d}}\lambda)\\
 & = & \exp(-b_{d}r^{_{d}}\Sigma_{i}\lambda)
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Plain Layout
